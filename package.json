{
  "name": "@source4society/react-scepter-redux-app",
  "version": "1.1.0",
  "description": "A universal app container that configures a connection to a redux store and react-router",
  "main": "lib/index.js",
  "repository": "https://github.com/source4societyorg/react-scepter-redux-app.git",
  "author": "Source 4 Society",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "test": "eslint ./src/*.js ./tests/*.js && jest --coverage",
    "build": "babel src/index.js --minified --out-file lib/index.js && babel src/props.js --minified --out-file lib/props.js",
    "prepublish": "npm run build"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 99,
        "branches": 99,
        "functions": 99,
        "lines": 99
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$"
  },
  "dependencies": {
    "@source4society/scepter-app-utilities": "^2.0.3",
    "react": "15.6.1",
    "react-redux": "^5.0.7",
    "react-router-redux": "5.0.0-alpha.6",
    "react-test-renderer": "15.6.1",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "^22.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-preset-env": "1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-redux-saga": "^0.6.0",
    "history": "^4.7.2",
    "jest-cli": "21.0.0",
    "react-dom": "^15.6.1"
  }
}
